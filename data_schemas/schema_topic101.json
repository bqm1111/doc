{
    "title": "Display",
    "description": "Debug (resized) image with information, including face (resized) bouding boxes, human (resized) bouding boxes \nShoule be use to draw in UI apps.",
    "type": "object",
    "properties": {
        "srctime": {
            "description": "Time stamp of this image",
            "type": "string",
            "format": "date-time"
        },
        "camera_id": {
            "description": "id of the camera that this event blong to",
            "type": "string"
        },
        "session_id": {
            "description": "unique id of deepstream-app session",
            "type": "string"
        },
        "frame_id": {
            "description": "frame id of this frame in this session",
            "type": "integer"
        },
        "frame": {
            "description": "base64 encoded of the resized full frame",
            "type": "string"
        },
        "frame_w": {
            "description": "width of the image",
            "type": "integer"
        },
        "frame_h": {
            "description": "height of the image",
            "type": "integer"
        },
        "FACE": {
            "description": "list of all faces in this image",
            "type": "array",
            "items": {
                "$ref": "#/definitions/FaceDisplayMeta"
            }
        },
        "MOT": {
            "description": "list of all mot object in this image",
            "type": "array",
            "items": {
                "$ref": "#/definitions/MotDisplayMeta"
            }
        }
    },
    "required": [
        "srctime",
        "camera_id",
        "session_id",
        "frame_id",
        "frame",
        "frame_w",
        "frame_h",
        "FACE",
        "MOT"
    ],
    "definitions": {
        "BBox": {
            "title": "BBox",
            "description": "Bouding boxes",
            "type": "object",
            "properties": {
                "x": {
                    "type": "number"
                },
                "y": {
                    "type": "number"
                },
                "w": {
                    "type": "number"
                },
                "h": {
                    "type": "number"
                }
            },
            "required": [
                "x",
                "y",
                "w",
                "h"
            ]
        },
        "FaceDisplayMeta": {
            "title": "FaceDisplayMeta",
            "description": "Face for display in debug mode",
            "type": "object",
            "properties": {
                "bbox": {
                    "description": "resized bouding box",
                    "allOf": [
                        {
                            "$ref": "#/definitions/BBox"
                        }
                    ]
                },
                "frame_w": {
                    "description": "width of frame which this bbox is on",
                    "type": "integer"
                },
                "frame_h": {
                    "description": "height of frame which this bbox is on",
                    "type": "integer"
                },
                "is_stranger": {
                    "description": "is this face a stranger?",
                    "type": "boolean"
                },
                "name": {
                    "description": "name of this person",
                    "type": "string"
                },
                "confident": {
                    "description": "face naming confident",
                    "type": "number"
                }
            },
            "required": [
                "bbox",
                "frame_w",
                "frame_h",
                "is_stranger",
                "name",
                "confident"
            ]
        },
        "MotDisplayMeta": {
            "title": "MotDisplayMeta",
            "description": "MOT object for display in debug mode",
            "type": "object",
            "properties": {
                "bbox": {
                    "description": "resized bouding box",
                    "allOf": [
                        {
                            "$ref": "#/definitions/BBox"
                        }
                    ]
                },
                "frame_w": {
                    "description": "width of frame which this bbox is on",
                    "type": "integer"
                },
                "frame_h": {
                    "description": "height of frame which this bbox is on",
                    "type": "integer"
                },
                "object_id": {
                    "description": "MOT object id",
                    "type": "integer"
                }
            },
            "required": [
                "bbox",
                "frame_w",
                "frame_h",
                "object_id"
            ]
        }
    }
}